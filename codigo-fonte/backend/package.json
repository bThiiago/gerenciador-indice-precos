{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "seed": "ts-node src/utils/scripts/seed.ts",
    "dropdb": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "createdb": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "cleardb": "yarn dropdb && yarn createdb",
    "resetdb": "yarn dropdb && yarn createdb && yarn seed",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.{js,ts,json,md}\"",
    "precommit": "yarn format && yarn lint"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.2.7",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/csv-parse": "^1.2.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "lint-staged": "14",
    "prettier": "^3.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.2.2"
  }
}
